@model int
  <style>
        .cuadrado-2 {
        width: 50px; 
        height: 50px; 
        background:white;
        float:left;
        }
        .izq{
            float:left;
            
        }
        .bloque{
            background:white;
            clear:left;
            float:left;
            
        }
        .nada{
            background:white;
        width: 50px; 
        height: 50px; 
        
        float:left;
        clear:left;

        }
        </style>


        @{
        var z=0;
        var b=Model-1;
        var valor=Model;
        }
        
    @for (int i = 0 ; i < Model ; i++)
    {
        @if(i<Model-1){
        <div class="nada"></div>
        }
		@for(int l=0;l<b-z-1;l++){
        <div class="cuadrado-2"></div>
        }
        z++;
        
        @for(int m=0;m<z;m++){
            @if(i==Model-1 && m==0){
                <img class="bloque" src="~/images/bloque.webp"   alt="#"   >  
                 
                @for(int x=0;x<Model-1;x++){
                <img class="izq" src="~/images/bloque.webp"   alt="#"   > 
            }
           
            }
            @if(i!=Model-1){
    
         <img class="izq" src="~/images/bloque.webp"   alt="#"   >  }      
        }
        
        }
    <br/>


           



